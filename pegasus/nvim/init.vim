set number
syntax on
"set listchars="    ":\|\ 
set list
colorscheme industry
highlight Normal ctermbg=none
highlight NonText ctermbg=none
set tabstop=4
set shiftwidth=4
set laststatus=2
set mouse=a
set clipboard=unnamed
"set clipboard=unnamedplus
"Plug 'bfredl/nvim-miniyank'
"map p <Plug>(miniyank-autoput)
"map P <Plug>(miniyank-autoPut)
